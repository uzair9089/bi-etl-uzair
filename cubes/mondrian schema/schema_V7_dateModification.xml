<Schema name="Demo" description="demo">
  <Cube name="Appointment" visible="true" description="Data from appointment facts" cache="true" enabled="true">
    <Table name="fact_appointments" schema="stage">
    </Table>
    <Dimension type="TimeDimension" visible="true" foreignKey="created_at_id" highCardinality="false" name="dim_date" description="date dimension">
      <Hierarchy name="Month Aggregation" visible="true" hasAll="true" description="Aggregation by month">
        <Table name="list_date" schema="stage">
        </Table>
        <Level name="Months" visible="true" table="list_date" column="created_at_id" nameColumn="calendar_month" ordinalColumn="month_id" parentColumn="calendar_year" type="String" uniqueMembers="true" levelType="TimeMonths" hideMemberIf="Never" description="Monthly Aggregates">
        </Level>
        <Level name="Year" visible="true" table="list_date" column="calendar_year" nameColumn="calendar_year" type="String" uniqueMembers="false" levelType="TimeYears" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="created_by_merchant_id" highCardinality="false" name="createdBy" description="Information about appoinments if they were created by merchant or not">
      <Hierarchy name="createdBy" visible="true" hasAll="true" description="Information about appoinments if they were created by merchant or not">
        <Table name="list_created_by_merchant" schema="stage">
        </Table>
        <Level name="createdBy" visible="true" table="list_created_by_merchant" column="id" nameColumn="description" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" description="Information about appoinments if they were created by merchant or not">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="appointment_cancelled_id" highCardinality="false" name="appointmentStatus" description="Status of the appointment if it is still active or cancelled">
      <Hierarchy name="appointmentStatus" visible="true" hasAll="true" description="Status of the appointment if it is still active or cancelled">
        <Table name="list_appointment_status" schema="stage">
        </Table>
        <Level name="appointmentStatus" visible="true" table="list_appointment_status" column="id" nameColumn="cancelled" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" description="Status of the appointment if it is still active or cancelled">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="origin_id" highCardinality="false" name="origin" description="origin of the appointments">
      <Hierarchy name="origin" visible="true" hasAll="true" description="Origin of the appointments">
        <Table name="list_origin" schema="stage">
        </Table>
        <Level name="origin" visible="true" table="list_origin" column="id" nameColumn="origin" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" description="Origin of the appointments">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="state_id" highCardinality="false" name="state" description="state of the appointment">
      <Hierarchy name="state" visible="true" hasAll="true" description="state of the appointment">
        <Table name="list_state" schema="stage">
        </Table>
        <Level name="state" visible="true" table="list_state" column="id" nameColumn="state" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" description="state of the appointment">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="location_id" highCardinality="false" name="location" description="Location of the appointment">
      <Hierarchy name="location" visible="true" hasAll="true" description="Location for the appointment">
        <Table name="list_location" schema="stage">
        </Table>
        <Level name="location" visible="true" table="list_location" column="id" nameColumn="address_country_code" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never" description="Location for appointment">
        </Level>
      </Hierarchy>
    </Dimension>
    <Measure name="Total Count" column="counter" datatype="Numeric" aggregator="sum" description="Gives the count related to each and every dimension" visible="true">
    </Measure>
    <Measure name="price" column="price" datatype="Numeric" aggregator="sum" description="price for the service" visible="true">
    </Measure>
  </Cube>
</Schema>
